<p id="notice"><%= notice %></p>

<h1>Listing Evaluations</h1>

<table>
  <thead>
    <tr>
      <th>Note</th>
      <th>Total score</th>
      <th>Participant</th>
      <th>Member</th>
      <th>Evaluation builder</th>
      <th>Event</th>
      <th>Organization</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluations.each do |evaluation| %>
      <tr>
        <td><%= evaluation.note %></td>
        <td><%= evaluation.total_score %></td>
        <td><%= evaluation.participant_id %></td>
        <td><%= evaluation.member_id %></td>
        <td><%= evaluation.evaluation_builder_id %></td>
        <td><%= evaluation.event_id %></td>
        <td><%= evaluation.organization_id %></td>
        <td><%= link_to 'Show', evaluation %></td>
        <td><%= link_to 'Edit', edit_evaluation_path(evaluation) %></td>
        <td><%= link_to 'Destroy', evaluation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluation', new_evaluation_path %>
