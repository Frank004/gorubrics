<div class="col-md-5 col-md-offset-3 col-lg-offset-3">
  <h1>Nuevo Evento</h1>
  <div class="authform well">
	<%= form_for([@organization,@event], :html => { :class => 'form' }) do |f| %>
	 <%= render 'form', f: f %>
	  <div class="actions">
	    <%= f.submit "Actualizar",  class: 'btn btn-warning' %>
    	<%= link_to 'regresar', organization_events_path(@organization),  class: 'btn btn-primary'  %>
	  </div>
	<% end %>
  </div>
</div>